{
  "name": "astrobook",
  "type": "module",
  "version": "0.4.4",
  "description": "The minimal UI component playground",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/astrobook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/astrobook.git"
  },
  "bugs": "https://github.com/ocavue/astrobook/issues",
  "keywords": [
    "ui",
    "component",
    "playground",
    "storybook",
    "react",
    "vue",
    "preact",
    "svelte",
    "solid",
    "lit",
    "astro",
    "vite"
  ],
  "sideEffects": false,
  "main": "./lib/index.ts",
  "module": "./lib/index.ts",
  "types": "./lib/index.ts",
  "exports": {
    ".": {
      "default": "./lib/index.ts"
    },
    "./client": {
      "default": "./lib/client.ts"
    },
    "./pages/*": {
      "default": "./lib/pages/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.ts"
      ],
      "./client": [
        "./lib/client.ts"
      ],
      "./pages/*": [
        "./lib/pages/*"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@astrobook/core": "workspace:*",
    "@astrobook/types": "workspace:*",
    "@astrobook/ui": "workspace:*"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "astro": "^4.15.6",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vitest": "^2.1.1"
  }
}
