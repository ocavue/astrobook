  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.ts",
  "module": "./dist/index.ts",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "default": "./dist/index.ts"
    },
    "./components/*": {
      "default": "./dist/components/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.ts"
      ],
      "./components/*": [
        "./dist/components/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx copy.ts src temp && unocss --write-transformed && tsx minify-inline-script.ts && tsx copy.ts temp dist"
  },
  "dependencies": {
    "@astrobook/core": "workspace:*",
    "@astrobook/types": "workspace:*",
    "astro-theme-toggle": "^0.3.5"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.7",
    "@iconify-json/mdi": "^1.2.0",
    "@types/node": "^20.16.10",
    "@unocss/cli": "^0.63.3",
    "astro": "^4.16.5",
