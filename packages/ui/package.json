{
  "name": "@astrobook/ui",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/astrobook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/astrobook.git"
  },
  "bugs": "https://github.com/ocavue/astrobook/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.ts",
  "module": "./dist/index.ts",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "default": "./dist/index.ts"
    },
    "./components/*": {
      "default": "./dist/components/*"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.ts"
      ],
      "./components/*": [
        "./dist/components/*"
      ],
      "./style.css": [
        "./dist/style.css"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rsync -av --delete src/ temp/ && unocss --write-transformed && rsync -av --delete temp/ dist/ && rm -rf temp"
  },
  "dependencies": {
    "@astrobook/core": "workspace:*",
    "@astrobook/types": "workspace:*"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.14.13",
    "@unocss/cli": "^0.61.9",
    "astro": "^4.13.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  }
}
