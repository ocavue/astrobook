{
  "name": "@astrobook/ui",
  "type": "module",
  "version": "0.3.0",
  "description": "",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/astrobook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/astrobook.git"
  },
  "bugs": "https://github.com/ocavue/astrobook/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.ts",
  "module": "./dist/index.ts",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "default": "./dist/index.ts"
    },
    "./components/*": {
      "default": "./dist/components/*"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.ts"
      ],
      "./components/*": [
        "./dist/components/*"
      ],
      "./style.css": [
        "./dist/style.css"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cpx 'src/**/*' temp/ && unocss --write-transformed && ./minify-inline-script.ts && cpx 'temp/**/*' dist/"
  },
  "dependencies": {
    "@astrobook/core": "workspace:*",
    "@astrobook/types": "workspace:*"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.1.205",
    "@iconify-json/mdi": "^1.1.68",
    "@types/node": "^20.14.14",
    "@unocss/cli": "^0.61.9",
    "astro": "^4.13.2",
    "cpx2": "^7.0.1",
    "esbuild": "^0.23.0",
    "fdir": "^6.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
}
