  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/astrobook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/astrobook.git"
  },
  "bugs": "https://github.com/ocavue/astrobook/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.ts",
  "module": "./dist/index.ts",
  "types": "./dist/index.ts",
  "exports": {
    ".": {
      "default": "./dist/index.ts"
    },
    "./components/*": {
      "default": "./dist/components/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.ts"
      ],
      "./components/*": [
        "./dist/components/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx copy.ts src temp && unocss --write-transformed && prettier -w temp/style.css && tsx copy.ts temp dist"
  },
  "dependencies": {
    "@astrobook/core": "workspace:*",
    "@astrobook/types": "workspace:*",
    "astro-theme-toggle": "^0.6.0",
    "just-group-by": "^2.2.0"
  },
  "peerDependencies": {
    "astro": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.46",
    "@iconify-json/mdi": "^1.2.3",
    "@types/node": "^22.15.29",
    "@unocss/cli": "^66.1.3",
    "astro": "^5.9.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unocss": "^66.1.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
}
