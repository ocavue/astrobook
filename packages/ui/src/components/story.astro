---
import '@astrobook/types'

import Layout from './layout.astro'
import StoryWrapper from './story-wrapper.astro'

interface Props {
  hasSidebar: boolean
  story: string
}
---

<Layout storyId={Astro.props.story} hasSidebar={Astro.props.hasSidebar}>
  <StoryWrapper story={Astro.props.story} />
</Layout>
