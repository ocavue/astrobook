{
  "name": "@astrobook/core",
  "type": "module",
  "version": "0.11.2",
  "description": "Internal package for Astrobook",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/astrobook#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/astrobook.git"
  },
  "bugs": "https://github.com/ocavue/astrobook/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./client": {
      "default": "./src/client.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "@astrobook/types": "workspace:*",
    "@sveltejs/acorn-typescript": "^1.0.8",
    "acorn": "^8.15.0",
    "fdir": "^6.5.0",
    "picomatch": "^4.0.3",
    "slash": "^5.1.0",
    "valibot": "^1.2.0",
    "yoctocolors": "^2.1.2"
  },
  "peerDependencies": {
    "astro": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "astro": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/picomatch": "^4.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^6.0.0-beta.1",
    "just-kebab-case": "^4.2.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./client": {
        "types": "./dist/client.d.ts",
        "import": "./dist/client.js",
        "default": "./dist/client.js"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
      "*": {
        ".": [
          "./dist/index.d.ts"
        ],
        "./client": [
          "./dist/client.d.ts"
        ]
      }
    }
  }
}
